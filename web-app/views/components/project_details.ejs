

  
      <!---->
      <section  id="Overview" class="mb-4">
        
        <div class="card">
          <div class="card-header text-center py-3">
            <h5 class="mb-0 text-center">
              <strong><%=project.Title%></strong>
            </h5>
          </div>
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-hover text-nowrap">
                <thead>
                  <tr>
                    <th scope="col"></th>
                    <th scope="col"></th>

                    <th scope="col">Description</th>
                    <th scope="col">DeathLine</th>
                    <th scope="col">Job Duty</th>

                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th scope="col"></th>
                    <th scope="col"></th>

                    <td><%=project.description%></td>
                    <td><%=project.Deathline%></td>
                    <td><%=contribute.Contribution_Role%></td>

                  </tr>
                
                </tbody>
              </table>
            </div>
            
        </div>
        <div class="row ">
          <div class="col-xl-6 col-md-12 mb-4">
            <div class="card">
              <div class="card-body">
                <div class="d-flex justify-content-between p-md-1">
                  <div class="d-flex flex-row">
                    <div class="align-self-center">
                      <i class="fas fa-pencil-alt text-info fa-3x me-4"></i>
                    </div>
                    <div>
                      <h4>Teams Contribution</h4>
                      <p class="mb-0">Base on this project</p>
                    </div>
                  </div>
                  <div class="align-self-center">
                    <h2 class="h1 mb-0"><%=totalteams%></h2>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-xl-6 col-md-12 mb-4">
            <div class="card">
              <div class="card-body">
                <div class="d-flex justify-content-between p-md-1">
                  <div class="d-flex flex-row">
                    <div class="align-self-center">
                      <i class="fas fa-pencil-alt text-info fa-3x me-4"></i>
                    </div>
                    <div>
                      <h4>Estimate Completed</h4>
                      <p class="mb-0">yy-mm-dd</p>
                    </div>
                  </div>
                  <div class="align-self-center">
                    <h2 class="h1 mb-0">2022-22-06</h2>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
          <div class="row">
            <div class="col-xl-6 col-md-12 mb-4">
              <div class="card">
                <% if(productbl){ %>
  
                <div class="card-body">
                  <div class="d-flex justify-content-between px-md-1">
                    <div>
                      <h3 class="text-info">Product Backlog</h3>
                     <div>
                     
                      <ol class="list-group list-group-numbered">
                        <% for ( let i = 0 ; i < productbl.length ; i++) { %>
  
                        <li class="list-group-item d-flex justify-content-between align-items-start">
                          <div class="ms-2 me-auto">
                            <div class="fw-bold"><%=productbl[i].description%></div>
                         
                          </div>
                          <span class="badge bg-primary rounded-pill"></span>
                        </li>
                        <% } %>
  
                      </ol>
                     </div>
                    </div>
                    <div class="align-self-center">
                      <i class="fas fa-book-open text-info fa-3x"></i>
                      
                    </div>
                  </div>
                
                </div>
                
                <% } %>
                <div class="card-footer bg-transparent border-success">

                  <form action="/addproductbl/" method='POST' class="row gy-2 gx-3 align-items-center">
                    <div class="col-auto">
                      <div class="form-outline">
                        <input type="hidden" name='projectid' value='<%=projectid%>' id="form11Example1" class="form-control" />

                        <input type="text" name='Description' id="form11Example1" class="form-control" />
                        <label class="form-label" for="form11Example1">New Product Item</label>
                      </div>
                    </div>
               
                 
                    <div class="col-auto">
                      <button type="submit" class="btn btn-primary">ADD</button>
                    </div>
                  </form>
                  
                </div>

              </div>
  
            </div>
            <div class="col-xl-6 col-md-12 mb-4">
              <div class="card">
                <% if(productbl){ %>
  
                <div class="card-body">
                  <div class="d-flex justify-content-between px-md-1">
                    <div>
                      <h3 class="text-danger">Sprint Backlog</h3>
                     <div>
                     
                      <ol class="list-group list-group-numbered">
                        <% for ( let i = 0 ; i < sprintbl.length ; i++) { %>
                          <%if(sprintbl[i].Status === 'I'){%>
                            <li class="list-group-item d-flex  bg-warning justify-content-between align-items-start">
                              <%}else{%>
                                <li class="list-group-item bg-success d-flex justify-content-between align-items-start">
                                  <%}%>
                          <div class="ms-2 me-auto">
                            <div class="fw-bold">User Story</div>
                            <%=sprintbl[i].Use_Stories%>
                          </div>
                          <span class="badge bg-primary rounded-pill">
                            Product Backlog Item:<%=sprintbl[i].PItem_id%>
                            <span class="badge bg-primary rounded-pill">
                              Iteration #: <%=sprintbl[i].Iteration_Number%>
                            </span>
                                                      </span>
                          
                          
                        </li>
                       
                        <% } %>
  
                      </ol>
                     </div>
                    </div>
                    <div class="align-self-center">
                      <i class="fas fa-book-open text-info fa-3x"></i>
                      
                    </div>
                  </div>
                 
                </div>
                <% } %>
                <div class="card-footer bg-transparent border-success">

                
                  <form  action='/addsprintbl/' method='POST' class="row row-cols-lg-auto g-3 align-items-center">
                    
                    
                       
                    <div class="col-12">
                      <label class="visually-hidden" for="inlineFormSelectPref">Preference</label>
                      <select class="select" name="item_id" required>
                        <option value="" disabled selected>Please Choose Product item</option>
                        <% for(let i =0 ;  i < productbl.length ; i++) {%>
                        <option value='<%=productbl[i].Item_id%>'><%=productbl[i].description%></option>
                        <% }%>
                 
                    </select>
                    </div>
                  
                    <div class="col-12">
                      <label class="visually-hidden" for="inlineFormInputGroupUsername">Username</label>
                      <div class="input-group">
                        <input
                          type="number"
                          name="Iteration_Number"
                          class="form-control"
                          id="inlineFormInputGroupUsername"
                          placeholder="Iteration_Number"
                        />
                      </div>
                    </div>
                    <div class="col-12">
                      <div class="form-outline">
                        <textarea class="form-control" name ="User_Stories"id="textAreaExample" rows="4"></textarea>
  <label class="form-label" for="textAreaExample">User Stories</label>
                      </textarea>
                      </div>
                    </div>
               
                
                  
                    <div class="col-12">
                      <button type="submit" class="btn btn-primary">ADD</button>
                    </div>
                  </form>
                  
                  
                </div>
              </div>
  
            </div>
          </div>
  
      </section>
  
      

